[Statedef 8000,反射ヘルパー待ち]
	type    = S
	movetype= I
	physics = S
	ctrl = 0
	velset = 0,0
	anim = Root,Anim
	sprpriority = -2

	[State 8000, anim同期]
		type = ChangeAnim
		Trigger1 = Anim != Root,Anim
		Trigger2 = Anim = Root,Anim
		Trigger2 = AnimElemNo(0) != Root,AnimElemNo(0)
		value = Root,Anim
		elem = Root,AnimElemNo(0)

	[State 8000, 位置固定]
		type = BindToRoot
		trigger1 = 1
		time = 1
		facing = 1
		pos = 0, 0

	[State 8000, 影,透過]
		type = AssertSpecial
		trigger1 = 1
		flag = noshadow
		flag2 = invisible

	[State 8000,]
		type = NotHitBy
		trigger1 = Root,StateNo = 900
		value = SCA
		time = 1

	[State 8000,反射設定A]
		type = NotHitBy
		trigger1 = Root,Var(20) = 0
		value = ,AT,AP
		time = 1

	[State 8000,反射設定T]
		type = NotHitBy
		trigger1 = Root,Var(20) = 1
		value = ,AA,AP
		time = 1

	[State 8000,反射設定P]
		type = NotHitBy
		trigger1 = Root,Var(20) = 2
		value = ,AA,AT
		time = 1

	[State 8000,反射設定OFF]
		type = NotHitBy
		trigger1 = Root,Var(20) = 5
		trigger1 = helper(20000),Var(7) = 0
		value = ,AA,AT,AP
		time = 1

	[State 8000, 上書き]
		type = HitOverride
		trigger1 = 1
		attr = SCA,AA
		slot = 1
		stateno = 8010
		time = 1

	[State 8000, ステート変更]
		type = ChangeState
		trigger1 = EnemyNear(Root,Var(40)),HitDefAttr = SCA,AT
		trigger1 = Root,P2BodyDist X < 80 
		Trigger1 = Root,Var(20) = 1
		value = 8020

	[State 8000, 上書き]
		type = HitOverride
		trigger1 = 1
		attr = SCA,AT
		slot = 2
		stateno = 8020
		time = 1

	[State 8000, 上書き]
		type = HitOverride
		trigger1 = 1
		attr = SCA,AP
		slot = 3
		stateno = 8030
		time = 1

	[State 8000,Destroy]
		type = DestroySelf
		trigger1 = MoveReversed
		trigger2 = Root,Var(9) != 0
		trigger3 = Root,MoveType = H
		trigger4 = Root,StateNo = 900
		trigger5=root,fvar(10)=0

