; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Tsuchimikado Motoharu"        ;Name of character
displayname = "Tsuchimikado Motoharu (WIP)" ;Name of character to display
versiondate = 04,02,2022    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.1          ;Version of M.U.G.E.N character works on (X.XX)
ikemenversion = 1
author = "DidiGetsRekt,Seb368"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
sprite  = Tsuchimikado.sff           ;Sprite
anim    = Tsuchimikado.air           ;Animation
cmd     = Tsuchimikado.cmd           ;Command set
cns     = Tsuchimikado.cns           ;Constants
sound   = Tsuchimikado.snd           ;Sound

;for mugen
st	= Tsuchimikado.cns           ;Constants
st1	= Tsuchimikado_N.cns           ;States
st2	= Tsuchimikado_S.cns           ;States
st3	= Tsuchimikado_H.cns           ;States
st4	= Tsuchimikado_Helper.cns           ;States
st5	= Tsuchimikado_ex.cns
stcommon= common.cns

;for ikemen go
st6	= Tsuchimikado_text.zss
font0	= jg.fnt

movelist=Tsuchimikado.dat



ai      = Tsuchimikado.ai            ;AI hints data (not used)

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
;x2 = 7 ;Hold Start and press button X for palette 7, etc.
;y2 = 8
;z2 = 9
;a2 = 10
;b2 = 11
;c2 = 12

; Arcade mode
[Arcade]
intro.storyboard =
ending.storyboard =
