[Info]
Name         = "neco-misaka"  ;キャラクターの名前（必要な項目なので省略不可）
DisplayName  = "neco-misaka"  ;M.U.G.E.Nで実際に表示される名前（省略可能）
VersionDate  = 04,15,2011     ;キャラクターの最終更新日（変えなくてもＯＫ）
MugenVersion = 04,15,2011     ;キャラクターが動作出来るM.U.G.E.Nの改訂版
Author       = "Elecbyte"     ;キャラクターの作成者（必要な項目なので省略不可）
Pal.Defaults = 1,2,3,4        ;ＣＰＵで出てきた時のカラーパレットの順番

;--| 必要なファイルの設定 |---------------------------------------------------

[Files]
Cmd     = necomisaka.cmd       ;コマンドファイルの登録
Cns     = necomisaka.cns       ;コンスタンツファイルの登録
St      = necomisaka.cns       ;ステートファイルの登録・最大１１個まで増やせます
                        ;（St,St0,St1,St2,St3,St4,St5,St6,St7,St8,St9）
                        ;「St」のみ必要な項目なので省略出来ません
St1	= -2.cns
StCommon = common.cns  ;コモンステートファイルの登録
Sprite  = necomisaka.sff       ;複数のＰＣＸ画像をまとめたスプライトファイルの登録
Anim    = necomisaka.air       ;アニメーションファイルの登録
Sound   = necomisaka.snd       ;複数のＷＡＶＥ音声をまとめたサウンドファイルの登録
Movelist = necomisaka_Movelist.dat

;--| カラーパレットファイル |-------------------------------------------------

Pal1 = act/m1p.act  ;Ａボタンで決定した色が使えます
Pal2 = act/m2p.act  ;Ｂ
Pal3 = act/m3p.act  ;Ｃ
Pal4 = act/m4p.act  ;Ｘ
Pal5 = act/m5p.act  ;Ｙ
Pal6 = act/m6p.act  ;Ｚ

Pal7 = act/m7p.act  ;Ｓ押しながらＡボタンで決定した色が使えます
Pal8 = act/m8p.act  ;Ｓ押しながらＢ
Pal9 = act/m9p.act  ;Ｓ押しながらＣ
Pal10= act/m10p.act  ;Ｓ押しながらＸ
Pal11= act/m11p.act  ;Ｓ押しながらＹ
;Pal12=           ;Ｓ押しながらＺ

;--| ストーリーボード設定 |---------------------------------------------------

[Arcade]
Intro.Storyboard =
Ending.Storyboard =

